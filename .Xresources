#include ".theme"

*font: xft:Neep:size=13

*boldFont:
*lineSpace:             10
*letterSpace:           0
*geometry:              50x20
*internalBorder:        5
*scrollBar:             false


! xft fonts settings
Xft.autohint:           0
Xft.lcdfilter:          lcddefault
Xft.hinting:            1
Xft.hintstyle:          hintslight
Xft.antialias:          1
Xft.rgba:               rgb

URxvt.perl-ext-common: config-reload
